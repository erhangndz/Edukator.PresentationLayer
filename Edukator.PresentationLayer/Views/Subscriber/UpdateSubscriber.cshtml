@model Edukator.EntityLayer.Concrete.Subscriber
@{
    ViewData["Title"] = "UpdateSubscriber";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="bg-light rounded h-100 p-4 col-xl-12">
    <h6 class="mb-4"> Abone Güncelleme Sayfası</h6>
    <form method="post">
        <div class="mb-3">
            <input type="hidden" asp-for="SubscriberID" />
            <label for="mail" class="form-label">E-Mail</label>
            <input type="text" class="form-control" id="mail" asp-for="Mail" />
        </div>



        <button type="submit" class="btn btn-success">Güncelle</button>
    </form>
</div>

