@model Edukator.EntityLayer.Concrete.AppRole
@{
    ViewData["Title"] = "UpdateRole";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Rol Güncelleme Sayfası</h3>

<form method="post">
    @Html.HiddenFor(x=>x.Id)
    <label>Rol Adı</label>
    <input type="text" class="form-control" asp-for="Name" />
    <br />
    <button class="btn btn-primary">Rolü Güncelle</button>


</form>

